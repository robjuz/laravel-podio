{
  "name": "robjuz/laravel-podio",
  "type": "library",
  "description": "Podio integration for Laravel 5+",
  "keywords": [
    "podio",
    "laravel"
  ],
  "homepage": "https://github.com/robjuz/laravel-podio",
  "license": "MIT",
  "authors": [
    {
      "name": "Nikolaj LÃ¸venhardt Petersen",
      "email": "nikolaj.lovenhardt@gmail.com",
      "homepage": "https://github.com/nikolajlovenhardt"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "robjuz/podio-php": "^4.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "laravel/framework": "~5.4",
    "codeclimate/php-test-reporter": "~0.3.0",
    "phpmd/phpmd": "~2.3.2",
    "squizlabs/php_codesniffer": "~2.5.1",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "robjuz\\LaravelPodio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "robjuz\\LaravelPodio\\ServiceProvider"
      ],
      "aliases": {
        "Podio": "robjuz\\LaravelPodio\\Facade"
      }
    }
  }
}
